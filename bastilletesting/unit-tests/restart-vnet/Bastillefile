# unit-tests/restart-vnet

ARG OPTIONS="-Vx"
ARG JAIL
ARG RELEASE
ARG IP
ARG INTERFACE

INCLUDE core/create --arg OPTIONS=${OPTIONS} --arg JAIL=${JAIL} --arg RELEASE=${RELEASE} --arg IP=${IP} --arg INTERFACE=${INTERFACE}

INCLUDE core/restart --arg OPTIONS="-x" --arg JAIL=${JAIL}

INCLUDE core/destroy-jail --arg OPTIONS="-afyx" --arg JAIL=${JAIL}
