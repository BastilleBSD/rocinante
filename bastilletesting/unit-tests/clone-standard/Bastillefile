# unit-tests clone-standard

ARG OPTIONS
ARG JAIL
ARG RELEASE
ARG IP
ARG INTERFACE
ARG NEW_NAME


INCLUDE core/create --arg OPTIONS=${OPTIONS} --arg JAIL=${JAIL} --arg RELEASE=${RELEASE} --arg IP=${IP} --arg INTERFACE=${INTERFACE}

INCLUDE core/clone --arg OPTIONS="-x" --arg JAIL=${JAIL} --arg NEW_NAME=${NEW_NAME} --arg IP=${IP}

INCLUDE core/destroy-jail --arg OPTIONS=${OPTIONS} --arg NEW_NAME=${NEW_NAME}


