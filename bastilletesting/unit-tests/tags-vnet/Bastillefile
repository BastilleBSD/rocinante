# unit-tests tags-vnet

ARG OPTIONS="-Vx"
ARG JAIL
ARG ACTION
ARG TAGS="prod,web"

INCLUDE core/create --arg OPTIONS=${OPTIONS} --arg JAIL=${JAIL} --arg RELEASE=${RELEASE} --arg IP=${IP} --arg INTERFACE=${INTERFACE}

INCLUDE core/tags --arg OPTIONS="-x" --arg JAIL=${JAIL} --arg ACTION=add --arg TAGS=${TAGS}

INCLUDE core/tags --arg OPTIONS="-x" --arg JAIL=${JAIL} --arg ACTION=delete --arg TAGS=${TAGS}

INCLUDE core/destroy-jail --arg OPTIONS="-axy" --arg JAIL=${JAIL}
