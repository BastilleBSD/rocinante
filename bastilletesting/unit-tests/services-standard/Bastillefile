# unit-tests/services-standard

ARG JAIL
ARG RELEASE
ARG IP
ARG INTERFACE

INCLUDE core/create --arg JAIL=${JAIL} --arg RELEASE=${RELEASE} --arg IP=${IP} --arg INTERFACE=${INTERFACE}

INCLUDE core/pkg --arg OPTIONS="-axy" --arg JAIL=${JAIL} --arg ARGS="install nginx"

INCLUDE core/service --arg OPTIONS="-x" --arg JAIL=${JAIL} --arg SERVICE="nginx" --arg ARGS="start"

INCLUDE core/destroy-jail --arg OPTIONS="-afxy" --arg JAIL=${JAIL}
