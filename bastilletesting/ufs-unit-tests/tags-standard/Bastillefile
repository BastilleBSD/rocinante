# unit-tests tags-standard

ARG JAIL
ARG ACTION
ARG TAGS
ARG IP
ARG RELEASE
ARG INTERFACE

INCLUDE core/create --arg JAIL=${JAIL} --arg RELEASE=${RELEASE} --arg IP=${IP} --arg INTERFACE=${INTERFACE}

INCLUDE core/tags --arg OPTIONS=${OPTIONS} --arg JAIL=${JAIL} --arg ACTION=add --arg TAGS=${TAGS}

INCLUDE core/tags --arg OPTIONS=${OPTIONS} --arg JAIL=${JAIL} --arg ACTION=delete --arg TAGS=${TAGS}

INCLUDE core/destroy-jail --arg OPTIONS="-afy" --arg JAIL=${JAIL}

